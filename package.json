  "description": "Vue.js Component Framework, build on top of vuetify",
  "author": {
    "name": "Antoine Milochau",
    "email": "amilochau@outlook.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "components",
    "vuetify"
  ],
  "license": "MIT",
  "scripts": {
